#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xavier/Trabajo/Freelancer/Costa Rica/Erick Villanueva/DigitalTrust/br_web_page/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/xavier/Trabajo/Freelancer/Costa Rica/Erick Villanueva/DigitalTrust/br_web_page/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
